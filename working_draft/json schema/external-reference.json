{
  "@context": {
    "@vocab": "http://cargo.iata.org#"
  },
  "type": "object",
  "properties": {
    "DocumentType": {
      "description": "Type of the referenced document . Refer UNEDIFACT 1001  e.g. 740 - Air Waybill",
      "type": "string"
    },
    "DocumentName": {
      "description": "If no DocumentType provided, name of the referenced document ",
      "type": "string"
    },
    "DocumentOriginator": {
      "description": "Document originator details and contacts",
      "$ref": "file:///company.json"
    },
    "DocumentID": {
      "description": "Unique document identifier",
      "type": "string"
    },
    "DocumentVersion": {
      "description": "Document version number",
      "type": "string"
    },
    "URI": {
      "description": "URI of document or logistics object",
      "type": "undefined"
    }
  }
}
